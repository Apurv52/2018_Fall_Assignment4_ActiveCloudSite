@model IEnumerable<IEXTrading.Models.Equity>

<h2>TopBuy</h2>
<hr />

<table class="table">
    <thead>
        <tr>
            <th>
                Symbol
            </th>
            <th>
                Name
            </th>
            <th>
                Change Percent
            </th>
            <th>
                Change
            </th>
            <th>
                High
            </th>
            <th>
                PE Ratio
            </th>
            <th>
                YTD Change
            </th>
            <th>
                52 Week High
            </th>
            <th>
                52 Week Low
            </th>
        </tr>
    </thead>

    <tbody>

        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.symbol)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.companyName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.changePercent)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.change)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.high)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.peRatio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ytdChange)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.week52High)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.week52Low)
            </td>
        </tr>
        }
        <tr>
            <td>
                <a id="dbopt" asp-controller="Home" asp-action="SaveGainer"><img src="~/images/button_save-data.png" /></a>
            </td>
        </tr>
    </tbody>
</table>
<script>
    alertDbSave(@ViewBag.dbSuccessEquity);
</script>
